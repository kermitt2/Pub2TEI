      <xs:annotation>
        <xs:documentation>supplies the value of a date or time in some standard form.</xs:documentation>
      </xs:annotation>
      <xs:simpleType>
        <xs:restriction>
          <xs:simpleType>
            <xs:list itemType="ns1:tei_data.word"/>
