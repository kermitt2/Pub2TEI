          <xs:attributeGroup ref="tei:tei_att.typed.attributes"/>
          <xs:attributeGroup ref="tei:tei_att.notated.attributes"/>
        </xs:extension>
      </xs:complexContent>
    </xs:complexType>
  </xs:element>
  <xs:element name="trash">
    <xs:annotation>
      <xs:documentation>(Trash) contains noise information to be ignored, typically from vectoriel images</xs:documentation>
    </xs:annotation>
    <xs:complexType mixed="true">
      <xs:sequence>
        <xs:element minOccurs="0" maxOccurs="unbounded" ref="tei:ref"/>
      </xs:sequence>
      <xs:attributeGroup ref="tei:tei_att.editLike.attributes"/>
      <xs:attributeGroup ref="tei:tei_att.datable.attributes"/>
      <xs:attributeGroup ref="tei:tei_att.naming.attributes"/>
    </xs:complexType>
  </xs:element>
