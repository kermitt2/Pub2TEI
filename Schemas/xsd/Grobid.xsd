  </xs:attributeGroup>
  <xs:attributeGroup name="tei_att.tableDecoration.attribute.role">
    <xs:attribute name="role" default="data">
      <xs:annotation>
        <xs:documentation>(role) indicates the kind of information held in this cell or in each cell of this row.
Suggested values include: 1] label; 2] data</xs:documentation>
      </xs:annotation>
      <xs:simpleType>
        <xs:union>
          <xs:simpleType>
            <xs:restriction base="xs:token">
              <xs:enumeration value="label">
                <xs:annotation>
                  <xs:documentation>labelling or descriptive information only.</xs:documentation>
                </xs:annotation>
              </xs:enumeration>
            </xs:restriction>
          </xs:simpleType>
          <xs:simpleType>
            <xs:restriction base="xs:token">
              <xs:enumeration value="data">
                <xs:annotation>
                  <xs:documentation>data values.</xs:documentation>
                </xs:annotation>
              </xs:enumeration>
            </xs:restriction>
          </xs:simpleType>
          <xs:simpleType>
            <xs:restriction base="xs:token">
              <xs:pattern value="[^\p{C}\p{Z}]+"/>
